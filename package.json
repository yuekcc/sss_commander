{
  "name": "sss_commander",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build:server": "esbuild --bundle --platform=node --target=node18 src/index.js --outdir=dist --format=iife --minify",
    "server": "node dist/index.js",
    "web": "vite",
    "build:web": "vite build"
  },
  "author": "yuekcc",
  "license": "MIT",
  "prettier": {
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "printWidth": 120,
    "quoteProps": "consistent",
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "env": {
      "es2022": true,
      "commonjs": true,
      "browser": true,
      "node": true,
      "worker": true,
      "mocha": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended",
      "plugin:prettier/recommended"
    ]
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.14",
    "esbuild": "^0.17.18",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.12.0",
    "mitt": "^3.0.0",
    "postcss": "^8.4.23",
    "prettier": "^2.0.0",
    "tailwindcss": "^3.3.2",
    "vite": "^4.3.5",
    "vue": "^3.3.2"
  },
  "dependencies": {
    "@tinyhttp/app": "^2.0.32",
    "formidable": "^3.0.0",
    "lodash": "^4.17.21",
    "sirv": "^2.0.3",
    "typed.js": "^2.0.16"
  }
}
